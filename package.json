{
  "name": "auth-practice",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:db": "docker exec -it integration-tests-prisma psql -U prisma tests",
    "test": "npm run docker:up && npx dotenv -e .env.test -- npx prisma migrate deploy && npx dotenv -e .env.test -- jest --runInBand --watch",
    "dev": "ENV=development nodemon --exec ts-node ./app.ts",
    "start": "ENV=production node ./dist/app.js",
    "build": "tsc && cp -r ./src/views ./dist/src/views",
    "clean": "rm -r ./dist",
    "generate": "npx dotenv -e .env.development -- npx prisma generate",
    "migrate": "npx dotenv -e .env.development -- npx prisma migrate dev",
    "resetdb": "npx dotenv -e .env.development -- nodemon prisma/reset.ts",
    "play": "npx dotenv -e .env.development -- nodemon playground.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.11.1",
    "@faker-js/faker": "^9.0.3",
    "@prisma/client": "^5.19.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-async-handler": "^1.2.0",
    "express-validator": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "lorem-ipsum": "^2.0.8",
    "morgan": "^1.10.0",
    "supertest": "^7.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.13",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.5.0",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "prisma": "^5.19.0",
