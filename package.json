{
  "name": "auth-practice",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "npx eslint .",
    "fmt": "npx prettier --write .",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:db": "docker exec -it integration-tests-prisma psql -U prisma tests",
    "docker:migrate": "npm run docker:up && npx dotenv -e .env.test -- npx prisma migrate deploy",
    "docker:rollback": "npm run docker:up && npx dotenv -e .env.test -- npx prisma migrate resolve --rolled-back",
    "docker:reset": "npm run docker:up && npx dotenv -e .env.test -- prisma migrate reset",
    "docker:clean": "docker compose down --rmi all --volumes --remove-orphans",
    "test": "npm run docker:migrate && npx dotenv -e .env.test -- vitest",
    "build": "tsc -p tsconfig.prod.json",
    "build:start": "ENV=production node ./dist/app.js",
    "build:clean": "rm -r ./dist",
    "db:generate": "npx dotenv -e .env.development -- npx prisma generate",
    "db:migrate": "npx dotenv -e .env.development -- npx prisma migrate dev",
    "db:reset": "npx dotenv -e .env.development -- nodemon prisma/reset.ts",
    "dev": "ENV=development nodemon --exec ts-node ./app.ts",
    "play": "npx dotenv -e .env.development -- nodemon playground.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.11.1",
    "@faker-js/faker": "^9.0.3",
    "@prisma/client": "^5.19.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-async-handler": "^1.2.0",
    "express-validator": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "lorem-ipsum": "^2.0.8",
    "morgan": "^1.10.0",
    "ofetch": "^1.4.1",
    "query-string": "^9.1.1",
    "supertest": "^7.0.0",
    "wilson-score-interval": "^2.1.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.5.0",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.14.0",
    "@typescript-eslint/parser": "^8.14.0",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.57.1",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3",
    "prisma": "^5.19.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "vitest": "^2.1.5"
  }
}
